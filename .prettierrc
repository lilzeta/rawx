{
    "arrowParens": "always",
    "bracketSpacing": true,
    "endOfLine": "lf",
    "insertPragma": false,
    "printWidth": 95,
    "proseWrap": "always",
    "quoteProps": "as-needed",
    "requirePragma": false,
    "semi": true,
    "singleQuote": false,
    "tabWidth": 4,
    "trailingComma": "all",
    "useTabs": false,
    "parser": "typescript"
}
